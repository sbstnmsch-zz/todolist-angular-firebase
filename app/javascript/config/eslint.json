{
  "parser": "babel-eslint",

  "env": {
    "browser": true
  },

  "globals": {
    "require": 1,
    "module": 1,
    "window": 1,
    "console": 1,
    "document": 1,
    "angular": 1
  },

  "rules": {
    "no-extra-strict": 0,
    "quotes": [2, "single"],
    "eqeqeq": 0,
    "strict": 0,
    "no-underscore-dangle": 0,
    "no-console": 0
  }
}
